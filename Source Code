#include<stdio.h>
#include<stdlib.h>

int factor(int a);

int main(void)
{
    int x;
    int *p;
    scanf("%i", &x);
    p = &x;
    factor(x);
}

int factor(int a)
{
    for (int i = 0; i < a;)
    {
        if (a == 1)
        {
            printf("%i", a);
            return 0;
        }
        else
        {
            i++;
            return a * factor(a - 1);
        }
    }
    return 0;
}
