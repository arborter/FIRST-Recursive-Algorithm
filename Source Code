#include<stdio.h>
#include<stdlib.h>

int factor(int a);

int main(void)
{
    int x;
    int *p;
    scanf("%i", &x);
    p = &x;
    factor(x);
}

int factor(int a)
{
    for (int i = 0; i < a; i++)
    {
        int b = a - 1;
        c = a*b;
        if (i == a - 1)
        {
            printf("%p", p)
            return true;
        }
        return true;
    }
    return false;
}
